s);\n          case \"danger\":\n            return toast.error(message, configs);\n        }\n      }\n\n    componentDidUpdate(){\n        let alerts = this.props.alerts || [];\n        alerts !== null &&\n        alerts.length > 0 &&\n        alerts.map((alert, idx) => {\n          this.createNotification(`${alert.alertType}`, alert.msg);\n        });\n    }\n\n    handleScroll(e) {\n      let elem = document.querySelector(\".navbar\");\n      if(document.getElementById('main').getBoundingClientRect().top < -55){\n        elem.classList.add('nav-sticky')\n      } else {\n        elem.classList.remove('nav-sticky')\n\n      }\n\n    }\n\n    render() {\n        console.log(this.props , \"sdbhbjh\");\n\n        \n        return (\n            <div>\n                <ToastContainer \n                    position=\"top-right\"\n                    autoClose={5000}\n                    hideProgressBar={false}\n                    newestOnTop={false}\n                    closeOnClick\n                    rtl={false}\n                    pauseOnFocusLoss = {false}\n                    draggable\n                    pauseOnHover\n                />\n                {/* <h1>{ this.state.title }</h1> */}\n                <Header />\n                <div id=\"main\" onScroll={(e) => this.handleScroll(e)}>\n                  <Switch>\n                      { routes.map( route => <Route key={ route.path } { ...route } /> ) }\n                  </Switch>\n                </div>\n                <Footer />\n            </div>\n        );\n    }\n}\n\nconst mapStateToProps = state => ({\n    alerts: state.alert,\n  });\n  \nexport default connect( mapStateToProps )( Layout );\n  \n","/home/dbws/projects/resume-maker/resume-react-ui/src/actions/alert.js",["218"],"import uuid from \"uuid\";\nimport { SET_ALERT, REMOVE_ALERT } from \"./types\";\n\nexport const setAlert = (\n  msg,\n  alertType,\n  err_key = \"\",\n  timeout = 5000\n) => dispatch => {\n  const id = Math.random(100);